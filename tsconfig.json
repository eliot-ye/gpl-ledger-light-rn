{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "exclude": [
    "JSONSchema/**"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  }
}